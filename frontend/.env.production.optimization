# Frontend - Optimizaciones de Render
# Agregala al frontend/.env.production

# API URL para producción
VITE_API_URL=https://empulse-api.onrender.com

# Configuración de caché
VITE_CACHE_SYMPTOMS=3600  # 1 hora en segundos
VITE_CACHE_NEWS=1800      # 30 minutos
VITE_CACHE_FORUM=900      # 15 minutos

# Paginación
VITE_SYMPTOMS_PER_PAGE=20
VITE_FORUM_POSTS_PER_PAGE=10
VITE_NEWS_PER_PAGE=5

# Timeouts
VITE_API_TIMEOUT=10000    # 10 segundos
